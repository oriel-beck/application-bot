{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "jsx": false,
            "dynamicImport": false,
            "privateMethod": false,
            "functionBind": false,
            "exportDefaultFrom": false,
            "exportNamespaceFrom": false,
            "decorators": true,
            "decoratorsBeforeExport": true,
            "topLevelAwait": false,
            "importMeta": false,
            "preserveAllComments": false
        },
        "transform": null,
        "target": "esnext",
        "loose": false,
        "externalHelpers": false,
        // Requires v1.2.50 or upper and requires target to be es2016 or upper.
        "keepClassNames": true
    },
    "isModule": true
}